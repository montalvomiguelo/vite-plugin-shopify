<hello-world>
  <a href="https://vitejs.dev" target="_blank" class="logo">
    {%- render 'icon-vite' -%}
  </a>
  <a href="https://shopify.dev/themes" target="_blank" class="logo">
    {%- render 'icon-shopify' -%}
  </a>
  <h1>Vite + Shopify</h1>
  <div class="card">
    <button id="counter" type="button">
      count is <output for="counter">0</output>
    </button>
  </div>
  <p class="read-the-docs">
    Click on the Vite and Shopify logos to learn more
  </p> 
</hello-world>

{% schema %}
{
  "name": "Hello world",
  "class": "w-100",
  "presets": [
    {
      "name": "Hello world"
    }
  ]
}
{% endschema %}